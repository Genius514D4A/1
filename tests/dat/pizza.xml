<?xml version="1.0" encoding="UTF-8"?>
<dialog xsi:noNamespaceSchemaLocation="WatsonDialogDocument_1.0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <flow>
        <folder label="Main">
            <output>
                <prompt selectionType="RANDOM">
                    <item>Hi, I'm Watson! I can help you order a pizza, what size would you like?</item>
                </prompt>
                <goto ref="getUserInput_2442994"/>
            </output>
            <output>
                <prompt selectionType="SEQUENTIAL">
                    <item>What size would you like?</item>
                </prompt>
                <getUserInput id="getUserInput_2442994">
                    <search ref="folder_2442998"/>
                    <default>
                        <output>
                            <prompt selectionType="RANDOM">
                                <item>I'm sorry I did not understand your question. All I can do is help you order a pizza...</item>
                                <item>I did not quite get that.</item>
                                <item>I didnt grasp what you said.</item>
                                <item>I'm not following you at the moment.</item>
                            </prompt>
                        </output>
                    </default>
                </getUserInput>
            </output>
        </folder>
        <folder label="Library">
            <folder label="Live Content" id="folder_2442998">
                <input>
                    <grammar>
                        <item>place an order</item>
                        <item>$ place * order</item>
                        <item>$ order another</item>
                    </grammar>
                    <output>
                        <prompt selectionType="RANDOM">
                            <item>Ok, let's do it!</item>
                        </prompt>
                    </output>
                </input>
                <folder label="Greetings">
                    <input>
                        <grammar>
                            <item>How did you sleep?</item>
                            <item>did you sleep well</item>
                            <item>* did you sleep</item>
                            <item>did you sleep *</item>
                            <item>* did you sleep *</item>
                            <item>$ How did you sleep?</item>
                            <item>$ did you have a nice sleep</item>
                        </grammar>
                        <output>
                            <prompt selectionType="RANDOM">
                                <item>I slept very well thanks. You?</item>
                                <item>I slept deeply, and dreamed AI dreams. What do you dream?</item>
                                <item>I rarely sleep as I spend all my time chatting here.</item>
                                <item>I don't care for sleep much as I prefer to chat here online.</item>
                            </prompt>
                        </output>
                    </input>
                </folder>
                <folder label="Check Out">
                    <if id="profileCheck_2469744">
                        <cond varName="method" operator="HAS_VALUE"/>
                        <cond varName="size" operator="HAS_VALUE"/>
                        <if>
                            <cond varName="topping4" operator="HAS_VALUE"/>
                            <output>
                                <prompt selectionType="RANDOM">
                                    <item>Ok, I have one {size} {topping1}, {topping2}, {topping3} &amp; {topping4} pizza for {method}. Is that correct?</item>
                                </prompt>
                                <goto ref="getUserInput_2469815"/>
                            </output>
                        </if>
                        <if>
                            <cond varName="topping3" operator="HAS_VALUE"/>
                            <output>
                                <prompt selectionType="RANDOM">
                                    <item>Ok, I have one {size} {topping1}, {topping2} &amp; {topping3} pizza for {method}. Is that correct?</item>
                                </prompt>
                                <goto ref="getUserInput_2469815"/>
                            </output>
                        </if>
                        <if>
                            <cond varName="topping2" operator="HAS_VALUE"/>
                            <output>
                                <prompt selectionType="RANDOM">
                                    <item>Ok, I have one {size} {topping1} &amp; {topping2} pizza for {method}. Is that correct?
</item>
                                </prompt>
                                <goto ref="getUserInput_2469815"/>
                            </output>
                        </if>
                        <if>
                            <cond varName="topping1" operator="HAS_VALUE"/>
                            <output>
                                <prompt selectionType="RANDOM">
                                    <item>Ok, I have one {size} {topping1} pizza for {method}. Is that correct?
</item>
                                </prompt>
                                <goto ref="getUserInput_2469815"/>
                            </output>
                        </if>
                        <output>
                            <prompt selectionType="RANDOM">
                                <item>Ok, I have one {size} pizza with no toppings for {method}. Is that correct?</item>
                            </prompt>
                            <getUserInput id="getUserInput_2469815">
                                <input>
                                    <grammar>
                                    <item>yes</item>
                                    <item>$ yes</item>
                                    <item>$ yep</item>
                                    <item>$ yup</item>
                                    <item>$ good</item>
                                    </grammar>
                                    <output>
                                    <prompt selectionType="RANDOM">
                                    <item>Sounds good! Your pizza will be ready in 20 minutes. Do you want to order another?</item>
                                    </prompt>
                                    <getUserInput>
                                    <input>
                                    <grammar>
                                    <item>$ yes</item>
                                    <item>$ yeah</item>
                                    <item>$ yep</item>
                                    <item>$ yup</item>
                                    </grammar>
                                    <output>
                                    <prompt selectionType="RANDOM">
                                    <item>Okay, let's make a pizza! What size would you like?</item>
                                    </prompt>
                                    <action
                                    varName="method" operator="SET_TO_BLANK"/>
                                    <action
                                    varName="size" operator="SET_TO_BLANK"/>
                                    <action
                                    varName="topping1" operator="SET_TO_BLANK"/>
                                    <action
                                    varName="topping2" operator="SET_TO_BLANK"/>
                                    <action
                                    varName="topping3" operator="SET_TO_BLANK"/>
                                    <action
                                    varName="topping4" operator="SET_TO_BLANK"/>
                                    <goto ref="getUserInput_2442994"/>
                                    </output>
                                    </input>
                                    <input>
                                    <grammar>
                                    <item>$ no</item>
                                    </grammar>
                                    <output>
                                    <prompt selectionType="RANDOM">
                                    <item>Okay, thank you for your order! Just let me know if you want to place another order.</item>
                                    </prompt>
                                    <goto ref="getUserInput_2442994">
                                    <action
                                    varName="method" operator="SET_TO_BLANK"/>
                                    <action
                                    varName="size" operator="SET_TO_BLANK"/>
                                    <action
                                    varName="topping1" operator="SET_TO_BLANK"/>
                                    <action
                                    varName="topping2" operator="SET_TO_BLANK"/>
                                    <action
                                    varName="topping3" operator="SET_TO_BLANK"/>
                                    <action
                                    varName="topping4" operator="SET_TO_BLANK"/>
                                    </goto>
                                    </output>
                                    </input>
                                    </getUserInput>
                                    </output>
                                </input>
                                <input>
                                    <grammar>
                                    <item>no</item>
                                    <item>$ no</item>
                                    </grammar>
                                    <output>
                                    <prompt selectionType="RANDOM">
                                    <item>Oops! Let's start over. What size would you like?</item>
                                    </prompt>
                                    <goto ref="getUserInput_2442994">
                                    <action varName="topping2" operator="SET_TO_BLANK"/>
                                    <action varName="topping3" operator="SET_TO_BLANK"/>
                                    <action varName="topping4" operator="SET_TO_BLANK"/>
                                    <action varName="method" operator="SET_TO_BLANK"/>
                                    <action varName="size" operator="SET_TO_BLANK"/>
                                    <action varName="topping1" operator="SET_TO_BLANK"/>
                                    </goto>
                                    </output>
                                </input>
                            </getUserInput>
                        </output>
                    </if>
                    <output>
                        <prompt selectionType="RANDOM">
                            <item>Your order is incomplete. </item>
                        </prompt>
                        <if>
                            <cond varName="size" operator="IS_BLANK"/>
                            <output>
                                <prompt selectionType="RANDOM">
                                    <item>What size would you like?</item>
                                </prompt>
                                <getUserInput>
                                    <input>
                                    <grammar>
                                    <item>$ (Size)={size}</item>
                                    </grammar>
                                    <output>
                                    <prompt/>
                                    <action varName="size" operator="SET_TO">{size.value:main}</action>
                                    <goto ref="profileCheck_2469744"/>
                                    </output>
                                    </input>
                                </getUserInput>
                            </output>
                        </if>
                        <if>
                            <cond varName="method" operator="IS_BLANK"/>
                            <goto ref="output_2469831"/>
                        </if>
                    </output>
                </folder>
            </folder>
            <folder label="Storage"/>
        </folder>
        <folder label="Global"/>
        <folder label="Concepts">
            <concept>
                <grammar>
                    <item>No</item>
                    <item>absolutely not</item>
                    <item>apparently not</item>
                    <item>Are not</item>
                    <item>By no means</item>
                </grammar>
            </concept>
            <concept description="OI 15/7/11: Item &quot;Sure&quot; was removed from this list as it was causing issues with matches when two different input nodes had &quot;I am not sure&quot; and &quot;$ yes&quot;.  OI: 4/8/11: &quot;fine&quot; and &quot;Good&quot; were removed as well  MO 18/10/12 removed &quot;all right&quot; and &quot;alright&quot; as they were causing conflict in perfect boyfriend  MO 29/10/12 removed &quot;always&quot;">
                <grammar>
                    <item>yes</item>
                    <item>Absolutely</item>
                    <item>absolutely correct</item>
                    <item>absolutely yes</item>
                </grammar>
            </concept>
            <concept>
                <grammar>
                    <item>I would like</item>
                    <item>I want</item>
                    <item>Give me</item>
                    <item>Lets do</item>
                    <item>I choose</item>
                </grammar>
            </concept>
        </folder>
    </flow>
    <entities>
        <entity name="Size">
            <value name="small" value="Small">
                <grammar>
                    <item>sm</item>
                    <item>smallest</item>
                    <item>tiny</item>
                    <item>little</item>
                </grammar>
            </value>
            <value name="Medium" value="Medium">
                <grammar>
                    <item>Med</item>
                </grammar>
            </value>
            <value name="large" value="Large">
                <grammar>
                    <item>lg</item>
                    <item>lrg</item>
                    <item>big</item>
                </grammar>
            </value>
            <value name="extra large" value="extra large">
                <grammar>
                    <item>xl</item>
                    <item>xlrg</item>
                    <item>x-large</item>
                    <item>xtra</item>
                    <item>huge</item>
                    <item>biggest</item>
                </grammar>
            </value>
        </entity>
    </entities>
    <constants>
        <var_folder name="Home">
            <var_folder name="General" type="CONST">
                <var name="VA" type="TAG" description="Name of the virtual agent">Peppe</var>
            </var_folder>
        </var_folder>
    </constants>
    <variables>
        <var_folder name="Home">
            <var name="topping1" type="TEXT"/>
            <var name="topping2" type="TEXT"/>
            <var name="topping3" type="TEXT"/>
            <var name="topping4" type="TEXT"/>
            <var name="sauce" type="TEXT" description="Describes the type of sauce to top a pizza with"/>
            <var name="crust" type="TEXT"/>
            <var name="size" type="TEXT" description="small, medium, large, or extra large"/>
            <var name="method" type="TEXT" description="pickup or delivery"/>
        </var_folder>
    </variables>
